{
  "name": "cancerhotspots-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.0.0",
    "lodash": "^4.17.5",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts-ts": "2.14.0",
    "react-table": "^6.8.0",
    "superagent": "^3.8.2"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "updateGenomeNexusAPI": "npm run fetchGenomeNexusAPI && npm run buildGenomeNexusAPI",
    "fetchGenomeNexusAPI": "curl http://www.genomenexus.org/v2/api-docs?group=internal | json > src/api/generated/genomeNexusAPIInternal-docs.json",
    "buildGenomeNexusAPI": "node scripts/generate-api.js src/api/generated GenomeNexusAPIInternal",
    "updateCancerHotspotsAPI": "npm run fetchCancerHotspotsAPI && npm run buildCancerHotspotsAPI",
    "fetchCancerHotspotsAPI": "curl http://www.cancerhotspots.org/v2/api-docs?group=cancer_hotspots | json > src/api/generated/cancerHotspotsAPI-docs.json",
    "buildCancerHotspotsAPI": "node scripts/generate-api.js src/api/generated CancerHotspotsAPI"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-table": "^6.7.7",
    "@types/superagent": "^3.5.7",
    "fs": "0.0.1-security",
    "json": "^9.0.6",
    "path": "^0.12.7",
    "request": "^2.85.0",
    "swagger-js-codegen": "https://github.com/cBioPortal/swagger-js-codegen.git#1b768600496db2415f956f6c4d8593bbb6e95297",
    "typescript": "^2.7.2"
  }
}
